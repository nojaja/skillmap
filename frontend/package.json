{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:sw && vue-tsc -b && vite build",
    "build:sw": "tsc -p tsconfig.sw.json",
    "preview": "vite preview",
    "test": "jest",
    "test:ci": "jest --coverage",
    "lint": "eslint ./src/**/*.ts --no-warn-ignored --no-ignore",
    "depcruise": "depcruise --config .dependency-cruiser.js src",
    "docs": "typedoc"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "pinia": "^3.0.4",
    "qrcode": "^1.5.4",
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@eslint/js": "^8.57.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.10.1",
    "@types/qrcode": "^1.5.6",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.23",
    "babel-jest": "^29.7.0",
    "dependency-cruiser": "^16.9.0",
    "eslint": "^8.57.0",
    "eslint-plugin-jsdoc": "^48.9.2",
    "eslint-plugin-sonarjs": "^0.23.0",
    "jest": "^29.6.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.28.0",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.18.2",
    "vite": "^7.2.4",
    "vue-tsc": "^3.1.4"
  }
