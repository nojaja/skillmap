{
  "extends": "./tsconfig.app.json",
  "compilerOptions": {
    "outDir": "./public",
    "rootDir": "./src/service-worker",
    "target": "ES2023",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "lib": ["ES2023", "DOM", "DOM.Iterable", "WebWorker"],
    "types": [],
    "noEmitOnError": true,
    "allowImportingTsExtensions": true,
    "verbatimModuleSyntax": true
  },
  "include": ["src/service-worker/**/*.ts"]
}